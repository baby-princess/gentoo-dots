set shell=/bin/bash

" plugins
call plug#begin('~/.vim/bundle')
Plug 'junegunn/goyo.vim'
Plug 'dylanaraps/wal'
Plug 'scrooloose/nerdtree'
Plug 'PProvost/vim-ps1'
Plug 'azadkuh/vim-cmus'
Plug 'aurieh/discord.nvim'
call plug#end()

set nocompatible              " be iMproved, required
filetype plugin on       " required
syntax on
set relativenumber
set wildmode=longest,list,full
set splitbelow splitright
colo wal
set path+=**

" All of your Plugins must be added before the following line
filetype plugin indent on    " required

" auto cd to file
autocmd BufEnter * silent! lcd %:p:h

" auto remove comments on new lines
autocmd FileType * setlocal formatoptions-=c formatoptions-=r formatoptions-=o

" saves session on exit so it can be restored later
autocmd BufWritePost * mksession! $HOME/.vim/session.vim 

" executes document hanlder script on write
autocmd BufWritePost * !~/.scripts/document-handler



" checks for the existance of a ~/.nvim/session.vim and sources it

function! Load_saved_session()
	if filereadable(expand("~/.vim/session.vim"))
		source $HOME/.vim/session.vim
	else
		echo '~/.vim/session.vim does not exist creating...'
		mksession '~/.vim/session.vim'
	endif
endfunction
" keybindings

" binds space and space to find the next <!!>, and deltes it , and finally
" puts you into insert mode
 
inoremap <Space><Space> <Esc>/<!!><Enter>"_c4l
vnoremap <space><space> <Esc>/<!!><Enter>"_c4l
nnoremap <space><space> <Esc>/<!!><Enter>"_c4l

" cmus controls with c and some other key
nnoremap cc :CmusCurrent<CR>
nnoremap cl :CmusNext<CR>
nnoremap ch :CmusPrevious<CR>
nnoremap ca :CmusPlay<CR>
nnoremap cu :CmusPause<CR>
nnoremap cs :CmusStop<CR>

" new buffer on space b press
nnoremap <space>b :badd<space>

"switch to the next buffer on B press
nnoremap B :w<CR>:bnext

" load saved session on <F2>
nnoremap <F2> :w<CR>:call Load_saved_session()<CR>
" allows for external copy pasting
vnoremap y <esc>"*y
vnoremap p <esc>"*p

" allows for splist to be used via ctrl hjkl
map <C-h> <C-w>h
map <C-j> <C-w>j
map <C-k> <C-w>k
map <C-l> <C-w>l

" opens the current document as a suckless senr presentation
nnoremap <F7> :!sent<Space><C-r>% 2>/dev/null &<CR><CR>
nnoremap <F8> $F@ly$:!feh --scale-down --auto-zoom --image-bg black <c-r>" &<CR><CR>

" indents on <F5> press
map <F5> gg=G

"spelling on <F9> press
map <F9> :set spell! spelllang=en_gb<CR>

" word count in visual mode with <F10>
vnoremap <F10> g<C-g>

" toggle paste mode with F11
set pastetoggle=<F11>

" auto vsplit and split with a keypress
nnoremap <space>s :split<space>~/
vnoremap <space>s <esc>:split<space>~/
nnoremap <space>v :vsplit<space>~/
vnoremap <space>v <esc>:vsplit<space>~/

" toggle nerd tree on ctrl n press
map <c-n> <esc>NERDTreeToggle<CR>

" on space t press in normal mode make new tab
nnoremap <space>t :tabfind<space>

" on capital T press go to next tab
nnoremap T :tabNext <CR>

" run document handler on current file with space c
nnoremap <space>c :w! \| !~/.scripts/document-handler <c-r>%<CR><CR>

" shell check on space h
nnoremap <space>h :!clear && shellcheck %<CR>

" search on S
nnoremap S :%s//g<Left><Left>

" auto swicth to goyo with space and g
nnoremap <space>g :Goyo<CR>

" urlscan on space u
nnoremap <space>u :w<CR>:!cat "%" \| urlscan -c -d<CR>


" R on space r
noremap <space>r :!R --no-save -q<CR>

